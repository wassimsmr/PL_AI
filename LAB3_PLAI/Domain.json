{
  "domain": "BLOCKS",
  "requirements": [":strips", ":typing"],
  "types": ["block"],
  "predicates": [
    {"name": "on", "parameters": [{"name": "?x", "type": "block"}, {"name": "?y", "type": "block"}]},
    {"name": "ontable", "parameters": [{"name": "?x", "type": "block"}]},
    {"name": "clear", "parameters": [{"name": "?x", "type": "block"}]},
    {"name": "handempty", "parameters": []},
    {"name": "holding", "parameters": [{"name": "?x", "type": "block"}]}
  ],
  "actions": [
    {
      "name": "pick-up",
      "parameters": [{"name": "?x", "type": "block"}],
      "precondition": {"and": [
        {"clear": ["?x"]},
        {"ontable": ["?x"]},
        {"handempty": []}
      ]},
      "effect": {"and": [
        {"not": {"ontable": ["?x"]}},
        {"not": {"clear": ["?x"]}},
        {"not": {"handempty": []}},
        {"holding": ["?x"]}
      ]}
    },
    {
      "name": "put-down",
      "parameters": [{"name": "?x", "type": "block"}],
      "precondition": {"holding": ["?x"]},
      "effect": {"and": [
        {"not": {"holding": ["?x"]}},
        {"clear": ["?x"]},
        {"handempty": []},
        {"ontable": ["?x"]}
      ]}
    },
    {
      "name": "stack",
      "parameters": [{"name": "?x", "type": "block"}, {"name": "?y", "type": "block"}],
      "precondition": {"and": [
        {"holding": ["?x"]},
        {"clear": ["?y"]}
      ]},
      "effect": {"and": [
        {"not": {"holding": ["?x"]}},
        {"not": {"clear": ["?y"]}},
        {"clear": ["?x"]},
        {"handempty": []},
        {"on": ["?x", "?y"]}
      ]}
    },
    {
      "name": "unstack",
      "parameters": [{"name": "?x", "type": "block"}, {"name": "?y", "type": "block"}],
      "precondition": {"and": [
        {"on": ["?x", "?y"]},
        {"clear": ["?x"]},
        {"handempty": []}
      ]},
      "effect": {"and": [
        {"holding": ["?x"]},
        {"clear": ["?y"]},
        {"not": {"clear": ["?x"]}},
        {"not": {"handempty": []}},
        {"not": {"on": ["?x", "?y"]}}
      ]}
    }
  ]
}
